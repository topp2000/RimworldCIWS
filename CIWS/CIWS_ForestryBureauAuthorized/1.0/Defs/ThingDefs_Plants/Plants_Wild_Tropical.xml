<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="TreeBase">
		<defName>PlantTieshu</defName>
		<label>铁树</label>
		<description>一种质地坚硬的树木，堪比钢铁（其实就是钢铁）。</description>
		<graphicData>
			<texPath>Things/Plant/TreeTieshu</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<statBases>
			<Beauty>5</Beauty>
			<Nutrition>1.25</Nutrition>
		</statBases>
		<plant>
			<growDays>5</growDays>
			<harvestedThingDef>Steel</harvestedThingDef>
			<harvestWork>400</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestYield>80</harvestYield>
			<harvestFailable>true</harvestFailable>
			<sowMinSkill>1</sowMinSkill>
			<visualSizeRange>
				<min>1.3</min>
				<max>2.4</max>
			</visualSizeRange>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/TreeTieshu</immatureGraphicPath>
			<sowResearchPrerequisites>
				<li>LinYeJuShouQuanA</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.15</topWindExposure>
			<mustBeWildToSow>false</mustBeWildToSow>
			<wildOrder>3</wildOrder>
			<purpose>Food</purpose>
			<interferesWithRoof>false</interferesWithRoof><!--屋顶干扰-->
		</plant>
	</ThingDef>


