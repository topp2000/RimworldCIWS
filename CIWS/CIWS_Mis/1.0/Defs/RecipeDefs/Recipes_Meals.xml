	<RecipeDef ParentName="CookMealBase">
		<defName>CookMealSurvivalDa</defName>
		<label>烹饪十份包装生存食品</label>
		<description>高品质的包装食品，生产以供极端生存的环境下食用。</description>
		<jobString>正在烹饪十份包装生存食品。</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>840</workAmount>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>AnimalProductRaw</li>
					</categories>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<products>
			<MealSurvivalPack>10</MealSurvivalPack>
		</products>
		<skillRequirements>
			<Cooking>8</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>PackagedSurvivalMeal</researchPrerequisite>
	</RecipeDef>

	<RecipeDef>
		<defName>CookLianxi</defName>
		<label>练习烹饪</label>
		<description>用两份简单食物来练习烹饪技能。</description>
		<jobString>正在练习烹饪。</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<targetCountAdjustment>25</targetCountAdjustment><!--目标数量调整-->
		<workAmount>1600</workAmount>
		<workSkillLearnFactor>25</workSkillLearnFactor><!--技能学习加成-->
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MealSimple</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MealSimple</li>
			</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>MealSimple</li>
			</thingDefs>
		</defaultIngredientFilter>
		<products>
			<MealSimple>2</MealSimple>
		</products>
		<workSkill>Cooking</workSkill>
	</RecipeDef>


