<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef ParentName="CookMealBase">
		<defName>CookMealSimpleDa</defName>
		<label>烹饪十份简易食物</label>
		<description>直接用一种原材料做出食物来。</description>
		<jobString>正在烹饪十份简易食物。</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>FoodRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<MealSimple>10</MealSimple>
		</products>
		<workSkill>Cooking</workSkill>
	</RecipeDef>

	<RecipeDef ParentName="CookMealBase">
		<defName>CookMealFineDa</defName>
		<label>烹饪十份精致食物</label>
		<description>用生肉配上生菜的组合作为原料来制作相对更复杂的食物。</description>
		<jobString>正在烹饪十份精致食物。</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>AnimalProductRaw</li>
					</categories>
				</filter>
				<count>2.5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>2.5</count>
			</li>
		</ingredients>
		<products>
			<MealFine>10</MealFine>
		</products>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
	</RecipeDef>

