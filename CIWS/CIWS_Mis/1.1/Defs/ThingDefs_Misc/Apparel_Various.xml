			<MoveSpeed>0.04</MoveSpeed>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Waist</li>
				<li>Arms</li>
				<li>Legs</li>
				<li>Neck</li>
				<li>FullHead</li>
				<li>Hands</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/SnowWhiteCoats/SnowWhiteCoats</wornGraphicPath>
			<layers>
				<li>OnSkin</li><!--贴身-->
				<li>Shell</li><!--外套-->
			</layers>
			<tags>
				<li>Outlander</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel" />

	</ThingDef>


</Defs>