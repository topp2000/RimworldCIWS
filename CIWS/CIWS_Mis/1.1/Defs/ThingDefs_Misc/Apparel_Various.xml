<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_SnowWhiteCoats</defName>
		<label>白雪公主的新衣</label>
		<description>白雪公主的新衣，只有集齐七个小矮人才能看见（雾），感谢lolidrop 的贴图支持。</description>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/SnowWhiteCoats/SnowWhiteCoats</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>80</costStuffCount>
		<costList>
			<Cloth>80</Cloth>
			<Silver>1</Silver>
			<CIWS_XuKeZheng>1</CIWS_XuKeZheng>
		</costList>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
			<MarketValue>800</MarketValue>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToMake>22000</WorkToMake>
			<Mass>2.2</Mass>
			<ArmorRating_Sharp>5.0</ArmorRating_Sharp>
			<ArmorRating_Blunt>5.0</ArmorRating_Blunt>
			<ArmorRating_Heat>5.0</ArmorRating_Heat>
