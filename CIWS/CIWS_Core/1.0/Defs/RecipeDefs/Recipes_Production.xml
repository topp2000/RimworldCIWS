	<RecipeDef ParentName="MakeStoneBlocksBase">
		<defName>MakeCIWSSandsAny</defName>
		<label>制作建筑沙</label>
		<description>用石头粉碎制作建筑用沙。</description>
		<jobString>正在制作建筑沙。</jobString>
		<workAmount>600</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<workSkillLearnFactor>0.3</workSkillLearnFactor>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<recipeUsers>
			<li>TableStonecutter</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>StoneBlocks</li>
					</categories>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>StoneBlocks</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<CIWSSands>40</CIWSSands>
		</products>
	</RecipeDef>

	<RecipeDef>
		<defName>MakeShengLiao</defName>
		<label>制作水泥生料</label>
		<description>制作水泥生料，用于煅烧核电水泥的原料。</description>
		<jobString>正在制作水泥生料。</jobString>
		<workAmount>1600</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BlocksLimestone</li>
					</thingDefs>
				</filter>
				<count>70</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>CIWSSands</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BlocksLimestone</li>
				<li>CIWSSands</li>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<ShuiNiShengLiao>10</ShuiNiShengLiao>
		</products>
		<recipeUsers>
			<li>TableStonecutter</li>
		</recipeUsers>
	</RecipeDef>

