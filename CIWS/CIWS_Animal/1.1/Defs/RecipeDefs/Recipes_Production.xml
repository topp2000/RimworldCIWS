		<description>处理金蛋。</description>
		<jobString>正在处理金蛋。</jobString>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>2500</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>EggJinChickenFertilized</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>EggJinChickenFertilized</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Gold>10</Gold>
		</products>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>CraftingSpot</li>
		</recipeUsers>
	</RecipeDef>

