<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>CIWS1130A</defName>
		<label>H/PJ-A型末端防御塔</label>
		<thingClass>Building_TurretGun</thingClass>
		<graphicData>
			<texPath>Things/Building/Security/TurretA</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Security/TurretA_Top</uiIconPath>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<stealable>false</stealable>
		<rotatable>false</rotatable>
		<thingCategories>
			<li>BuildingsSecurity</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Flammability>0</Flammability>
			<WorkToBuild>2500</WorkToBuild>
			<Mass>120</Mass>
			<Beauty>-10</Beauty>
			<ShootingAccuracyTurret>0.97</ShootingAccuracyTurret>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>1</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<chanceNeverExplodeFromDamage>0.5</chanceNeverExplodeFromDamage>
			</li>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>1700</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Refuelable"><!--消耗弹药-->
				<fuelLabel>弹药数量</fuelLabel>
				<fuelGizmoLabel>弹药数量</fuelGizmoLabel>
				<fuelFilter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</fuelFilter>
				<fuelCapacity>1000</fuelCapacity><!--弹药总数-->
				<initialFuelPercent>1</initialFuelPercent><!--初始装填率-->
				<autoRefuelPercent>0.75</autoRefuelPercent><!--弹药总量比例-->
				<showFuelGizmo>true</showFuelGizmo>
				<minimumFueledThreshold>1</minimumFueledThreshold><!--最小燃油阈值-->
				<fuelMultiplier>25</fuelMultiplier><!--装填乘数，实际消耗资源=弹药总数×弹药总量比例/装填乘数-->
				<factorByDifficulty>true</factorByDifficulty>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
				<outOfFuelMessage>不能射击：需要装弹</outOfFuelMessage>
				<fuelIconPath>UI/Overlays/Barrel</fuelIconPath>
			</li>
		</comps>
		<description>H/PJ-1130A自动末端武器系统，殖民地的最后一层防御。</description>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<fillPercent>0.4</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<hasTooltip>true</hasTooltip>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>175</costStuffCount>
		<costList>
			<Steel>350</Steel>
			<ComponentIndustrial>3</ComponentIndustrial>
			<Shimoxi>10</Shimoxi>
		</costList>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Security</designationCategory>
		<specialDisplayRadius>12</specialDisplayRadius>
		<building>
			<ai_combatDangerous>true</ai_combatDangerous>
			<turretGunDef>HPJ_14A</turretGunDef>
			<turretBurstCooldownTime>0.01</turretBurstCooldownTime>
			<turretTopGraphicPath>Things/Building/Security/TurretA_Top</turretTopGraphicPath>
		</building>
		<researchPrerequisites>
			<li>GunTurrets</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BaseBullet">
