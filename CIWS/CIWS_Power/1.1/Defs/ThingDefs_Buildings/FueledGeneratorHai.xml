	<ThingDef ParentName="BuildingBase">
		<defName>FueledGeneratorHai</defName>
		<label>核聚变发电站</label>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Power/FueledGeneratorHai</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(10,6)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<pathCost>150</pathCost>
		<blockWind>true</blockWind>
		<fillPercent>1.0</fillPercent>
		<castEdgeShadows>false</castEdgeShadows>
		<rotatable>false</rotatable>
		<!--旋转选项-->
		<statBases>
			<MaxHitPoints>3000</MaxHitPoints>
			<WorkToBuild>5000</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<tickerType>Normal</tickerType>
		<description>可控稳定核聚变装置，可以安全稳定的输出电力。</description>
		<size>(10,6)</size>
		<costList>
			<Steel>1200</Steel>
			<Plasteel>300</Plasteel>
			<CIWS_ShuiNi>150</CIWS_ShuiNi>
			<ComponentIndustrial>20</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerPlant</compClass>
				<basePowerConsumption>-1000000</basePowerConsumption>
				<transmitsPower>true</transmitsPower>
			</li>
			<li Class="CompProperties_Flickable" />
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.832</fuelConsumptionRate>
				<fuelCapacity>100.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Hai3Yetai</li>
					</thingDefs>
				</fuelFilter>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>15</glowRadius>
				<glowColor>(135,206,250,0)</glowColor>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>120</heatPerSecond>
				<heatPushMaxTemperature>50</heatPushMaxTemperature>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
